{
    "name": "mbjonesua/laravel-user-proxy",
    "description": "Laravel User Proxy",
    "version": "0.2",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mbjonesua\\LaravelUserProxy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mbjonesua\\LaravelUserProxy\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mbjonesua\\LaravelUserProxy\\LaravelUserProxyServiceProvider"
            ],
            "aliases": {
                "LaravelUserProxy": "Mbjonesua\\LaravelUserProxy\\LaravelUserProxyFacade"
            }
        }
    }

   
}
